<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <stroke android:width="@dimen/_1sdp" android:color="@color/grey"/>
            <size
                android:width="@dimen/_20sdp" android:height="@dimen/_20sdp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <stroke android:color="@color/colorGreenCalendarSelect" android:width="@dimen/_1sdp"/>
            <size
                android:width="@dimen/_20sdp" android:height="@dimen/_20sdp" />
        </shape>
    </item>
</selector>